<!-- brightcove -->
<script type="text/javascript">
	var importBrightcove = function() {
		var loaded = 0;
		var wait = function() {
			loaded++;
			if(loaded == 2) {
				brightcove.createExperiences();
			}
		}
		var protocol = window.location.protocol;
		var base;
		if(protocol == 'https:') {
			base = protocol + '//sadmin.brightcove.com';
			var bc = document.getElementsByClassName('bcxp');
			for(i = 0; i < bc.length; i++) {
				var sc = document.createElement('param');
				sc.name = 'secureConnections';
				sc.value = true;
				var sh = document.createElement('param');
				sh.name = 'secureHTMLConnections';
				sh.value = true;
				bc[i].appendChild(sc);
				bc[i].appendChild(sh);
			}
		} else {
			base = protocol + '//admin.brightcove.com';
		}
		var smp = document.createElement('script');
		var exp = document.createElement('script');
		smp.src = base + '/js/api/SmartPlayerAPI.js';
		exp.src = base + '/js/BrightcoveExperiences.js';
		document.documentElement.firstChild.appendChild(smp);
		document.documentElement.firstChild.appendChild(exp);
		smp.onload = function() { wait() }
		exp.onload = function() { wait() }
	};

	jQuery(window).on('load', function() {
		setTimeout(function() {
			importBrightcove();
		}, 0);
	});

	var players = [], videoPlayers = [], firedMetrics = [], prev;

	var _matchEID = function(eid, obj, field) {
		var match;
		for(i in obj) {
			if(typeof field != 'undefined') {
				if(obj[i][field] == eid) {
					match = i;
				}
			}
		}
		return match;
	};

	var onTemplateLoad = function(eid) {
		var player = brightcove.api.getExperience(eid);

		if(!_matchEID(eid,players,'id')) {
			players.push(player); 
		}
	};

	var onTemplateReady = function(evt) {
		var i = _matchEID(evt.target.experience.id, players, 'id');
		videoPlayers[i] = players[i].getModule(brightcove.api.modules.APIModules.VIDEO_PLAYER);
		firedMetrics[i] = { "0": false, "25": false, "50": false, "75": false, "100": false };

		videoPlayers[i]._addEventListener(brightcove.api.events.MediaEvent.PROGRESS, videoProgress);
		videoPlayers[i]._addEventListener(brightcove.api.events.MediaEvent.BEGIN, videoBegin);
		videoPlayers[i]._addEventListener(brightcove.api.events.MediaEvent.COMPLETE, videoComplete);
	};

	var videoProgress = function(e) {
		var i = _matchEID(e.target.experience.id, players, 'id');
		var pos = e.position/e.duration;
		var dur = Math.round(e.duration);
		var percent = Math.floor(pos*100);

		if(percent >= 25 && prev < 25 && !firedMetrics[i]["25"]) {
			firedMetrics[i]["25"] = true;
			trackDigitalData({
				video: {
					segmentTime: dur / 4
				},
				event: {
					action: '25% complete'
				}
			}, 'video-segments');
		} else if(percent >= 50 && prev < 50 && !firedMetrics[i]["50"]) {
			firedMetrics[i]["50"] = true;
			trackDigitalData({
				video: {
					segmentTime: dur / 4
				},
				event: {
					action: '50% complete'
				}
			}, 'video-segments');
		} else if(percent >= 75 && prev < 75 && !firedMetrics[i]["75"]) {
			firedMetrics[i]["75"] = true;
			trackDigitalData({
				video: {
					segmentTime: dur / 4
				},
				event: {
					action: '75% complete'
				}
			}, 'video-segments');
		}
		prev = percent;
	};

	var videoBegin = function(e) {
		trackDigitalData({
			video: {
				videoName: e.media.displayName
			}, 
			event: {
				action: 'video start'
			}
		}, 'video-segments');
	};

	var videoComplete = function(e) {
		trackDigitalData({
			video: {
				segmentTime: dur / 4
			},
			event: {
				action: 'video complete'
			}
		}, 'video-segments');
	};

</script>



<script type="text/javascript" src="/campaignlibs/etc/designs/ford_com/gtreveal/clientlibs.all.min.js"></script>












































































<script type="text/javascript">
jQuery(function($) {
    $.fn.prebind = function (type, selector, data, fn) {
        this.each(function () {
            var $this = $(this);

            $this.on(type, selector, data, fn);

            var currentBindings = $._data(this, 'events')[type];
            if ($.isArray(currentBindings)) {
                currentBindings.unshift(currentBindings.pop());
            }
        });
        return this;
    };
    window.eventTracking = window.eventTracking || [];
    window.digitaldata = window.digitaldata || {};
    window.trackDigitalData = function(ext, trackname) {

        try {
            // extends event object into digitaldata
            var data = $.extend(digitaldata, ext);

            // console.log(data);
            eventTracking.push({e: trackname, data: data});

            if(trackname) {
                _satellite.track(trackname);
            }
        } catch(e) {}
    };

    
});
</script>

<script type="text/javascript">
    try {
        jQuery(function($) {
            $( window ).load(function() {
	var lineupClicked = false;

	// Dropdown menu
	$('div.sticker-left').one('click', function(event){

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:subnav:content action:ford bronco',
				onclick: 'reveal:subnav:expand',
				contentType: 'reveal:module:subnav'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-nav');
	});

	// Dropdown menu items
	$('ul.sub-sticker-nav').on('click', 'a:not([dtm-tracked])', function(event){
		$(this).attr('dtm-tracked', true);

		var tText = $(this).text().toLowerCase();
		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:subnav:content action:ford bronco',
				onclick: 'reveal:subnav:' + tText,
				contentType: 'reveal:module:subnav'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-nav');
	});

	// Billboard controls
	$('section.billboard div.bx-controls').one('click', 'a', function(event){
		event.stopPropagation();

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:carousel scroll',
				contentType: 'reveal:none:billboard'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});

	// Introduction hotspots
	$('section.introduction').on('click', 'a.onClickImage:not([dtm-tracked])', function(event){
		$(this).attr('dtm-tracked', true);

		var cat;
		var href = $(this).attr('href').replace('#','');
		var elems = $(this).parents('section').find('div.gallery-item');

		$(elems).each(function(index, elem){
			if ($(elem).attr('data-gid').indexOf(href) > -1) {
				cat = $(elem).children('div.top-gallery-mobile').text().trim().toLowerCase();
			}
		});

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:hotspot',
				contentType: 'reveal:none:hotspot 2',
				contentSubType: cat
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});

	// Gallery images
	$('section.gallery').one('click', 'a.onClickImage', function(event){

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:photo click',
				contentType: 'reveal:none:gallery'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});

	// Gallery more
	$('section.gallery').one('click', 'a.ClickLoadMore', function(event){

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:view more',
				contentType: 'reveal:none:gallery more'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');

		// Fix for gallery white space issue
		//$( window ).trigger('scroll');
	});

	// Feature carousel controls
	$('section#cluster').one('click', 'a', function(event){
		event.stopPropagation();

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:carousel scroll',
				contentType: 'reveal:none:freedom to control'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});

	// Performance technology gallery controls
	$('section#performance div.blue-slide').one('click', 'a', function(event){

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:carousel scroll',
				contentType: 'reveal:none:performance & technology'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});

	// Video gallery videos
	$('section.video-gallery div.carousel-video').one('click', 'a.onClickImage', function(event){
		event.stopPropagation();

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:video view larger',
				contentType: 'reveal:module:video'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});

	// Video gallery controls
	$('section.video-gallery div.bx-controls').one('click', 'a', function(event){
		event.stopPropagation();

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:carousel scroll',
				contentType: 'reveal:module:video'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});

	// Engine hotspots
	$('section.engine').on('click', 'a.onClickImage:not([dtm-tracked])', function(event){
		$(this).attr('dtm-tracked', true);

		var cat;
		var href = $(this).attr('href').replace('#','');
		var elems = $(this).parents('section').find('div.gallery-item');

		$(elems).each(function(index, elem){
			if ($(elem).attr('data-gid').indexOf(href) > -1) {
				cat = $(elem).children('div.top-gallery-mobile').text().trim().toLowerCase();
			}
		});

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:hotspot',
				contentType: 'reveal:none:hotspot 1',
				contentSubType: cat
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});

	// Lineup controls	
	$('section.performance-lineup div.blue-slide').on('click', 'a', function(event){
		event.stopPropagation();

		if ($(this).hasClass('hidden-xs') || lineupClicked){
			return;
		}

		lineupClicked = true;
		
		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:carousel scroll',
				contentType: 'reveal:module:performance line-up'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});
	
		// Feature carousel gallery controls
	$('section.cluster div.ford-carousel').one('click', 'a', function(event){

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:carousel scroll',
				contentType: 'reveal:none:freedom to control'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});

	// Wheel controls
	$('section#wheels div.blue-slide').one('click', 'a', function(event){

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:carousel scroll',
				contentType: 'reveal:none:customize'
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});

	// Share form
	$('#main').on('click', 'span.ico-right', function(event){

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:share:launch',
				onclick: 'share:launch',
			}
		}

		trackDigitalData(newDigitalData, 'share-launch');
	});

	// Share social channels
	$('#main').on('click', 'a.social-share-ctrl', function(event){
		event.stopPropagation();

		var sChannel = $(this).attr('data-name').toLowerCase();
		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:share',
				onclick: 'share start:' + sChannel + ':asset'
			}
		}

		trackDigitalData(newDigitalData, 'share-start');
	});

	// Share email
	$('#main').on('click', 'a.gt-email-share-anchor', function(event){
		event.stopPropagation();

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:share',
				onclick: 'share start:email:asset'
			}
		}

		trackDigitalData(newDigitalData, 'share-start');
	});

	// Download
	$('#main').on('click', 'a.ico-right', function(event){

		var hrefArr = $(this).attr('href').split('/');
		var sDownload = hrefArr[hrefArr.length-1];

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:download',
				onclick: 'download:' + sDownload,
			}
		}

		trackDigitalData(newDigitalData, 'global-download');
	});

	// More... in copy blocks
	$('#main').on('click', 'a.more:not([dtm-tracked])', function(event){
		$(this).attr('dtm-tracked', true);

		var sSection = $(this).parents('section').attr('class');

		// Transform section class name into the expected value
		switch(sSection) {
			case 'billboard': sSection = 'intro'; break;
			case 'introduction': sSection = 'hotspot intro'; break;
			case 'gallery': sSection = 'gallery'; break;
			case 'configurator': sSection = 'configurator'; break;
			case 'performance': sSection = 'features'; break;
			case 'engine': sSection = 'hotspot engine'; break;
			case 'featured': sSection = 'aerodynamics'; break;		
			case 'ford-performance': sSection = 'racing'; break;
			case 'cluster': sSection = 'cluster'; break;
			default: sSection = sSection;
		}

		// The video gallery class may contain multiple values	
		if (sSection.indexOf('video-gallery') > -1) {
			sSection = 'video'
		}

		var newDigitalData = {
			onclick: {
				onclickLinkName: 'fv:reveal:content:action:ford bronco',
				onclick: 'reveal content:read more',
				contentType: 'reveal:module:' + sSection
			}
		}

		trackDigitalData(newDigitalData, 'reveal-onclick');
	});
});
        });
    } catch(e) {
        console.warn("Error adding custom DTM events");
    }
</script>


