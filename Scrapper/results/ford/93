            <span class="page-count-media-overlay"><%= index + 1 %> of <%= count %></span>
        </div>
        <% } %>

        <% } %>
        <div class="carousel-details-toggle hide-on-mobile-ford <%= (item.expandDetails ? 'default-expand' : '') %>">
            <a class="details-toggle is-show" tabindex="0" role="button" data-fgx-keypress="true">
                <span class="cta">View Details</span>
                <i class="chevron icon-action-chevron-down-25px" aria-hidden="true"></i>
